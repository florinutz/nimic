<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app.class">Symfony\Component\Console\Application</parameter>
        <parameter key="search.command.class">Flo\Nimic\Console\Command\SearchCommand</parameter>
    </parameters>

    <services>

        <service id="command.search" class="%search.command.class%">
            <tag name="command" />
        </service>

    </services>
</container>
